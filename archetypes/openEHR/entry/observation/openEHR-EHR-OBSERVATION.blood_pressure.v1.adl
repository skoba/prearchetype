						}
					}
					CLUSTER[at1033] occurrences matches {0..1} matches {	-- 部位
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0014] occurrences matches {0..1} matches {	-- 計測部位
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0025, 	-- 右腕
											at0026, 	-- 左腕
											at0027, 	-- 右足
											at0028, 	-- 左脚
											at1020, 	-- 右手首
											at1021, 	-- 左手首
											at1026, 	-- 右足首
											at1031, 	-- 左足首
											at1032, 	-- 指
											at1051, 	-- 足先
											at1053]	-- 動脈内
										}
									}
								}
							}
							ELEMENT[at1034] occurrences matches {0..1} matches {	-- 特別な部位
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at1035] occurrences matches {0..1} matches {	-- 方法
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at1036, 	-- 聴診
									at1037, 	-- 触診
									at1039, 	-- 機械
									at1040]	-- 侵襲的
								}
							}
						}
					}
					ELEMENT[at1038] occurrences matches {0..1} matches {	-- 平均同脈圧の計算式
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at1010] occurrences matches {0..1} matches {	-- 拡張期終末
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at1011, 	-- 4期
									at1012]	-- 5期
								}
							}
						}
					}
					allow_archetype CLUSTER[at1025] occurrences matches {0..1} matches {	-- 測定機器
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
				}
			}
		}
	}

